<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Sebaran"
        android:textSize="30dp"
        android:textColor="#000000"
        android:layout_marginTop="20dp"
        android:layout_centerInParent="true"
        android:layout_alignParentTop="true"
        android:id="@+id/tv_halaman">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total Kasus"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_halaman"
        android:layout_centerInParent="true"
        android:id="@+id/tv_total">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Positif"
        android:id="@+id/tv_positif"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_total"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="123456"
        android:id="@+id/tv_jumlahPositif"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_positif"
        >
    </TextView>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Sembuh"
    android:id="@+id/tv_sembuh"
    android:textSize="20dp"
    android:layout_marginTop="30dp"
    android:textColor="#000000"
    android:layout_below="@+id/tv_total"
    android:layout_centerInParent="true"
    >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12345"
        android:id="@+id/tv_angkaSembuh"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_sembuh"
        android:layout_centerInParent="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meninggal"
        android:id="@+id/tv_meninggal"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_total"
        android:layout_alignParentRight="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1236"
        android:id="@+id/tv_angkaMeninggal"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_meninggal"
        android:layout_alignParentRight="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total Kasus Hari Ini"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_angkaMeninggal"
        android:layout_centerInParent="true"
        android:id="@+id/tv_today">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Positif"
        android:id="@+id/tv_todayPositif"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_today"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="123456"
        android:id="@+id/tv_jumlahPositifToday"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_todayPositif"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sembuh"
        android:id="@+id/tv_todaySembuh"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_today"
        android:layout_centerInParent="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12345"
        android:id="@+id/tv_angkaSembuhToday"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_todaySembuh"
        android:layout_centerInParent="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meninggal"
        android:id="@+id/tv_todayMeninggal"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_today"
        android:layout_alignParentRight="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1234"
        android:id="@+id/tv_angkaMeninggalToday"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_todayMeninggal"
        android:layout_alignParentRight="true"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Informasi"
        android:textSize="20dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_below="@+id/tv_angkaMeninggalToday"
        android:layout_centerInParent="true"
        android:id="@+id/tv_informasi">
    </TextView>

     <TextView
         android:layout_height="wrap_content"
         android:layout_width="wrap_content"
         android:clickable="true"
         android:onClick="onClick"
         android:textSize="20dp"
         android:id="@+id/tv_gejala"
         android:textColor="#000000"
         android:layout_margin="20dp"
         android:layout_below="@id/tv_informasi"
         android:text="Gejala">
     </TextView>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:clickable="true"
        android:onClick="onClick"
        android:textSize="20dp"
        android:id="@+id/tv_pencegahan"
        android:textColor="#000000"
        android:layout_margin="20dp"
        android:layout_below="@id/tv_informasi"
        android:layout_centerInParent="true"
        android:text="Pencegahan">
    </TextView>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:clickable="true"
        android:onClick="onClick"
        android:textSize="20dp"
        android:id="@+id/tv_perawatan"
        android:textColor="#000000"
        android:layout_margin="20dp"
        android:layout_below="@id/tv_informasi"
        android:layout_alignParentRight="true"
        android:text="Perawatan">
    </TextView>



</RelativeLayout>